{
    "name": "@dhis2-chap/chap-lib",
    "version": "2.1.0",
    "module": "./build/es/index.js",
    "types": "./build/types/index.d.ts",
    "license": "MIT",
    "devDependencies": {
        "@dhis2/cli-app-scripts": "^12.2.0",
        "@types/css-modules": "^1.0.5",
        "@types/node": "^22.10.1",
        "typescript": "^5.7.2"
    },
    "scripts": {
        "watch": "tsc --watch",
        "build": "d2-app-scripts build && tsc --emitDeclarationOnly --outDir ./build/types",
        "start": "d2-app-scripts start",
        "test": "d2-app-scripts test",
        "deploy": "d2-app-scripts deploy",
        "generate": "yarn openapi --input ./public/openapi.json --output ./src/httpfunctions",
        "generate-localhost": "yarn openapi --input http://localhost:8000/v1/openapi.json --output ./src/httpfunctions"
    },
    "main": "./build/cjs/index.js",
    "exports": {
        "import": "./build/es/index.js",
        "require": "./build/cjs/index.js",
        "types": "./build/types/index.d.ts"
    },
    "dependencies": {
        "@dhis2/maps-gl": "^4.0.1",
        "@dhis2/multi-calendar-dates": "^2.1.1",
        "@dhis2/ui": "^10.1.10",
        "openapi-typescript-codegen": "^0.29.0"
    }
}
